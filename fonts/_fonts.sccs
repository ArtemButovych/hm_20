@font-face {
	font-family: "Bebas Neue Book";
	src: url("../fonts/bebas-neue-book/bebas-neue-book.eot"); /* IE9 Compat Modes */
	src: url("../fonts/bebas-neue-book/bebas-neue-book.eot?#iefix") format("embedded-opentype"), /* IE6-IE8 */
		url("../fonts/bebas-neue-book/bebas-neue-book.otf") format("opentype"), /* Open Type Font */
		url("../fonts/bebas-neue-book/bebas-neue-book.svg") format("svg"), /* Legacy iOS */
		url("../fonts/bebas-neue-book/bebas-neue-book.ttf") format("truetype"), /* Safari, Android, iOS */
		url("../fonts/bebas-neue-book/bebas-neue-book.woff") format("woff"), /* Modern Browsers */
		url("../fonts/bebas-neue-book/bebas-neue-book.woff2") format("woff2"); /* Modern Browsers */
	font-weight: normal;
	font-style: normal;
}